<h1>Editing Book</h1>
<%= form_with model: @book, url: edit_book_path(@book.id), method: :patch do |f| %>
  <p>Title</p>
  <%= f.text_field :title %>
  <p>Body</params>
  <%= f.text_area :body %>
  <%= f.submit 'Update Book' %>
  <%= link_to "Show", books_path(@book.id) %>
  <p>
    <a href="/books">Back</a>
  </p>
<% end %>
